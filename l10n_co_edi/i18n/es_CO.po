#. module: l10n_co_edi_jorels
#: model:mail.template,body_html:l10n_co_edi_jorels.email_template_radian
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;"
"\">\n"
"                        Dear\n"
"                        <t t-if=\"object.move_id and object.move_id."
"partner_id and object.move_id.partner_id.parent_id\">\n"
"                            <t t-out=\"object.move_id.partner_id.name or "
"''\">Brandon Freeman</t> (<t t-out=\"object.move_id.partner_id.parent_id."
"name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-if=\"object.move_id and object.move_id."
"partner_id\">\n"
"                                <t t-out=\"object.move_id.partner_id.name "
"or ''\">Brandon Freeman</t>,\n"
"                            </t>\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Here is your\n"
"                        <t t-if=\"object.event_id and object.event_id."
"name\">\n"
"                            Radian event <strong t-out=\"object.event_id."
"name or ''\">Acuse de recibido</strong>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            Radian event\n"
"                        </t>\n"
"                        <t t-if=\"object.edi_number\">\n"
"                            with number <strong t-out=\"object.edi_number "
"or ''\">E0310</strong>\n"
"                        </t>\n"
"                        <t t-if=\"object.move_id and object.move_id."
"number_formatted\">\n"
"                            (with invoice: <t t-out=\"object.move_id."
"number_formatted or ''\">INV0</t>)\n"
"                        </t>\n"
"                        <t t-if=\"object.company_id.business_name\">\n"
"                            from <t t-out=\"object.company_id.business_name "
"or ''\">YourCompany</t>.\n"
"                        </t>\n"
"                        <t t-else=\"object.company_id.name\">\n"
"                            from <t t-out=\"object.company_id.name or "
"''\">YourCompany</t>.\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Do not hesitate to contact us if you have any "
"questions.\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;"
"\">\n"
"                        Querido\n"
"                        <t t-if=\"object.move_id and object.move_id."
"partner_id and object.move_id.partner_id.parent_id\">\n"
"                            <t t-out=\"object.move_id.partner_id.name or "
"''\">Brandon Freeman</t> (<t t-out=\"object.move_id.partner_id.parent_id."
"name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-if=\"object.move_id and object.move_id."
"partner_id\">\n"
"                                <t t-out=\"object.move_id.partner_id.name "
"or ''\">Brandon Freeman</t>,\n"
"                            </t>\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Aquí está su\n"
"                        <t t-if=\"object.event_id and object.event_id."
"name\">\n"
"                            Evento del Radian <strong t-out=\"object."
"event_id.name or ''\">Acuse de recibido</strong>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            Evento del Radian\n"
"                        </t>\n"
"                        <t t-if=\"object.edi_number\">\n"
"                            con número <strong t-out=\"object.edi_number or "
"''\">E0310</strong>\n"
"                        </t>\n"
"                        <t t-if=\"object.move_id and object.move_id."
"number_formatted\">\n"
"                            (con factura: <t t-out=\"object.move_id."
"number_formatted or ''\">INV0</t>)\n"
"                        </t>\n"
"                        <t t-if=\"object.company_id.business_name\">\n"
"                            de <t t-out=\"object.company_id.business_name "
"or ''\">YourCompany</t>.\n"
"                        </t>\n"
"                        <t t-else=\"object.company_id.name\">\n"
"                            de <t t-out=\"object.company_id.name or "
"''\">YourCompany</t>.\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        No dude en contactarnos si tiene alguna inquietud.\n"
"                    </p>\n"
"                </div>\n"
"            "

#. module: l10n_co_edi_jorels
#: model:mail.template,body_html:l10n_co_edi_jorels.email_template_edi
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;"
"\">\n"
"                        Dear\n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or "
"''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or "
"''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or "
"''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Here is your\n"
"                        <t t-if=\"object.name\">\n"
"                            invoice <strong t-out=\"object.name or "
"''\">INV/2021/05/0005</strong>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            invoice\n"
"                        </t>\n"
"                        <t t-if=\"object.invoice_origin\">\n"
"                            (with reference: <t t-out=\"object."
"invoice_origin or ''\">SUB003</t>)\n"
"                        </t>\n"
"                        amounting in <strong t-out=\"format_amount(object."
"amount_total, object.currency_id) or ''\">$ 143,750.00</strong>\n"
"                        from <t t-out=\"object.company_id.name or "
"''\">YourCompany</t>.\n"
"                        <t t-if=\"object.payment_state in ('paid', "
"'in_payment')\">\n"
"                            This invoice is already paid.\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            Please remit payment at your earliest "
"convenience.\n"
"                            <t t-if=\"object.payment_reference\">\n"
"                                <br/><br/>\n"
"                                Please use the following communication for "
"your payment: <strong t-out=\"object.payment_reference or "
"''\">INV/2021/05/0005</strong>.\n"
"                            </t>\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Do not hesitate to contact us if you have any "
"questions.\n"
"                        <t t-if=\"object.invoice_user_id.signature\">\n"
"                            <br/>\n"
"                            <t t-out=\"object.invoice_user_id.signature or "
"''\">--<br/>Mitchell Admin</t>\n"
"                        </t>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;"
"\">\n"
"                        Querido\n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or "
"''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or "
"''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or "
"''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Aquí está su\n"
"                        <t t-if=\"object.name\">\n"
"                            factura <strong t-out=\"object.name or "
"''\">INV/2021/05/0005</strong>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            factura\n"
"                        </t>\n"
"                        <t t-if=\"object.invoice_origin\">\n"
"                            (con referencia: <t t-out=\"object."
"invoice_origin or ''\">SUB003</t>)\n"
"                        </t>\n"
"                        por un valor de <strong t-"
"out=\"format_amount(object.amount_total, object.currency_id) or ''\">$ "
"143,750.00</strong>\n"
"                        de <t t-out=\"object.company_id.name or "
"''\">YourCompany</t>.\n"
"                        <t t-if=\"object.payment_state in ('paid', "
"'in_payment')\">\n"
"                            Esta factura ya se encuentra pagada.\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            Por favor remita su pago tan pronto como le sea "
"posible.\n"
"                            <t t-if=\"object.payment_reference\">\n"
"                                <br/><br/>\n"
"                                Por favor use la siguiente referencia al "
"realizar su pago: <strong t-out=\"object.payment_reference or "
"''\">INV/2021/05/0005</strong>.\n"
"                            </t>\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        No dude en contactarnos si tiene alguna inquietud.\n"
"                        <t t-if=\"object.invoice_user_id.signature\">\n"
"                            <br/>\n"
"                            <t t-out=\"object.invoice_user_id.signature or "
"''\">--<br/>Mitchell Admin</t>\n"
"                        </t>\n"
"                    </p>\n"
"                </div>\n"
"            "

